  },
  "homepage": "https://github.com/tsq-cli/translate-server#readme",
  "dependencies": {
    "@google-cloud/translate": "^3.0.1",
    "axios": "^0.18.0",
    "bcrypt": "^3.0.6",
    "blueimp-md5": "^2.10.0",
    "chalk": "^2.4.2",
    "compression": "^1.7.4",
    "connect-mongo": "^2.0.3",
    "cors": "^2.8.5",
    "dotenv": "^7.0.0",
    "ejs": "^2.6.1",
    "express": "^4.16.4",
    "express-addon-response": "^1.0.0",
