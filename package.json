{
  "name": "translate-server",
  "version": "1.0.0",
  "description": "api server for translate cli",
  "main": "index.js",
  "scripts": {
    "test": "mocha",
    "prod": "node index.js",
    "start": "nodemon",
    "run": "docker-compose up",
    "apidoc": "apidoc -i docs/ -o ./public/apidoc",
